
class VGLUG:
    def __init__(self,name,age,id,,phone):
        self.name=name
        self.age=age
        self.id=id
        
        self.phone=phone

    def get member Details(self):
        print("name:",self.name)
        print("Age:",self.age)
        print("ID:",self.id)
        print("Phone:",self.phone)

Vcom=VGLUG("HARI",21,1014,750000,7094975269)
Vcom.get member Details()
